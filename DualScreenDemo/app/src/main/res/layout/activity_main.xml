<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/btn_presentation_mediarouter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/presentation_mediarouter" />

    <Button
        android:id="@+id/btn_presentation_displaymanager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/presentation_displaymanager" />

    <Button
        android:id="@+id/btn_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/set_launch_display_id" />


</LinearLayout>